{
    "manifest_version": 3,
    "name": "Genyu Token Pool",
    "version": "8.0",
    "description": "Auto-generate and pool reCAPTCHA tokens",
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "cookies"
    ],
    "host_permissions": [
        "https://*.google.com/*",
        "https://*.google/*",
        "https://labs.google/*",
        "http://localhost:3001/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}